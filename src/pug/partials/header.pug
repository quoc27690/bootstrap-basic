- const navItem = ['Home', 'About', 'Services']

header
  nav.navbar.navbar-expand-md.navbar-dark.bg-dark.sticky-top
    .container-fluid
      a.navbar-brand(href='#')
        img(src='./assets/images/logo1.png' height='40')
      button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarToggler')
        span.navbar-toggler-icon

      #navbarToggler.collapse.navbar-collapse
        ul.navbar-nav.mr-auto
          each item in navItem
            li.nav-item.active
              a.nav-link(href='#')= item
        form.form-inline
          input.form-control(type='search' placeholder='Search')
          button.btn.btn-outline-success.my-2.my-sm-0(type='submit')
            | Search
